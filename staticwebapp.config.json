{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/api/*",
      "/.auth/*",
      "*.{js,css,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot,json,xml}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
